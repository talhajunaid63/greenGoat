<%= semantic_form_for resource, url: add_schedule_admin_prospect_path(resource), method: :put do |f| %>
  <%= f.inputs do %>
    <%= f.input :visit_date, as: :date_picker, required: true %>
  <% end %>
  <%= f.actions do %>
    <%= f.action :submit, label: 'Add visit date' %>
    <%= link_to 'Cancel', admin_prospect_path(resource) %>
  <% end %>
<% end %>
