<table>
  <thead>
    <th>Title</th>
    <th>Status</th>
    <th>Uninstallation Date</th>
    <th>Actions</th>
  </thead>

  <tbody>
    <% @items.each do |item| %>
      <tr>
        <td><%= item.title %></td>
        <td><%= item.product_statuses.last %></td>
        <td><%= item.uninstallation_date&.strftime('%d-%m-%Y') %></td>
        <td><%= link_to "view", admin_item_path(item) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to "Go Back", admin_reports_path %>
